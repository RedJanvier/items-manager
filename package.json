{
  "name": "items-manager",
  "version": "1.0.0",
  "description": "Manage Items with this simple API",
  "main": "index.js",
  "scripts": {
    "start": "node src",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed:all",
    "db:init": "(dropdb --if-exists siyubu && createdb siyubu) && npm run migrate && npm run seed",
    "dev": "nodemon --exec babel-node src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RedJanvier/items-manager.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RedJanvier/items-manager/issues"
  },
  "homepage": "https://github.com/RedJanvier/items-manager#readme",
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.4.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "brace-style": [
            "error",
            "stroustrup"
        ],
        "comma-dangle": [
            "error",
            "never"
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-var": [
            "off"
        ],
        "one-var": [
            "off"
        ]
    }
}
}
